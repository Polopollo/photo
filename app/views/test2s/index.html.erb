<h1>Listing test2s</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Value</th>
  </tr>

<% @test2s.each do |test2| %>
  <tr>
    <td><%=h test2.name %></td>
    <td><%=h test2.value %></td>
    <td><%= link_to 'Show', test2 %></td>
    <td><%= link_to 'Edit', edit_test2_path(test2) %></td>
    <td><%= link_to 'Destroy', test2, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New test2', new_test2_path %>
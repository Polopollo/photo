.block
  .secondary-navigation
    %ul.wat-cf
      %li.first
        = link_to t("web-app-theme.list"), admin_printer_types_path
      %li
        = link_to t("web-app-theme.new"), new_admin_printer_type_path
      %li.active
        = link_to t("web-app-theme.edit"), edit_admin_printer_type_path
  .content
    %h2.title
      = t("web-app-theme.edit", :default => "Edit")
      PrinterType
    .inner
      - form_for @printer_type, :url => admin_printer_type_path(@printer_type), :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}
      -
- content_for :sidebar, render(:partial => 'sidebar')

.block
  .secondary-navigation
    %ul.wat-cf
      %li.first
        = link_to "#{t("web-app-theme.list", :default => "List")}", admin_printer_types_path
      %li.active
        = link_to "#{t("web-app-theme.new", :default => "New")}", new_admin_printer_type_path
  .content
    %h2.title
      = t("web-app-theme.new", :default => "New")
      PrinterType
    .inner
      - form_for :printer_type, :url => admin_printer_types_path, :html => { :class => :form } do |f|
      = render :partial => "form", :locals => {:f => f}
      - end
- content_for :sidebar, render(:partial => 'sidebar')

.block
  .secondary-navigation
    %ul.wat-cf
      %li.first
        = link_to t("web-app-theme.list"), admin_paper_types_path
      %li.active
        = link_to t("web-app-theme.new"), new_admin_paper_type_path
  .content
    %h2.title
      = t("web-app-theme.new")
      = t "site.paper_type.genitive.one"
    .inner
      - form_for :paper_type, :url => admin_paper_types_path, :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}
      -
- content_for :sidebar, render(:partial => 'sidebar')
